<h1>ユーザーログイン</h1>
<%= form_with url:'/login',local:true,scope: :session do |form| %>
<div>
<%= form.label :name %>
  <%= form.text_field :name %></div>
<div>
  <%= form.label :email %>
  <%= form.text_field :email %>
</div>
<div>
  <%= form.label :password %>
  <%= form.password_field :password %>
</div>
<div>
  <%= form.submit :送信%>
</div>
<% end %>
